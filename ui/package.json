{
  "name": "fam-asset",
  "version": "0.0.1",
  "description": "e-Asset Telkom Indonesia",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --port 80",
    "start:hmr": "ng serve --hmr",
    "start:aot": "ng serve --aot",
    "build:ci": "npm-run-all -p -r build:prod build:prod:aot",
    "build:play": "ng build -ec -pr -scd -op ../public/ui",
    "build:play:prod": "ng build -oh none -ec -pr --prod --aot -cc -vc -scd --build-optimizer -op ../public/ui",
    "test:play": "ng test -sr -sm false -cc",
    "lint": "ng lint --type-check",
    "lint:ci": "npm run lint && npm run lint:styles",
    "lint:fix": "ng lint --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "precommit": "npm run lint:fix && npm run lint",
    "prepush": "npm run lint:ci"
  },
